            android:layout_marginTop="8dp"
            layout="@layout/guest_item_shimmer_list"/>

        <include
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            layout="@layout/guest_item_shimmer_list"/>

        <include
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
